InstallScript := Func(PartFrame, RemoveFrame)Begin	If GlobalFnExists('RegisterDevice) then	// See if the function is loaded.		RegisterDevice(kAppSymbol, kNE2KInfoFrame);	else		GetRoot():Notify(kNotifyAlert, "Error!", "Unable to activate NE2K since Newton Device Drivers are not in the system");End;RemoveScript := Func(removeFrame)Begin	UnRegUserConfigChange(kAppSymbol);	If GlobalFnExists('UnRegisterDevice) then // See if the function is loaded.		UnRegisterDevice(kAppSymbol);End;